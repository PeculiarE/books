{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "describe": "readonly",
        "it": "readonly",
        "logger": "readonly",
        "before": "readonly",
        "after": "readonly"
      },
    "rules": {
        "no-underscore-dangle": 0,
        "no-param-reassign": 0
    }
}
